unit Pairs;

interface

uses
  Sysutils, customers, vehicles, Math;

  type
  Pair = Class

  private
  pID:integer;
  pv1,pv2 : vehicle;
  pline: customer;

  published
    constructor Create(ID:integer; v1:vehicle; v2:vehicle; line:customer);
    property ID: integer read pID write pID;
    property v1: vehicle read pv1 write pv1;
    property v2: vehicle read pv2 write pv2;
    property line: customer read pline write pline;

  End;


implementation

end.
