unit Demands;

interface

type
   demand = Class

     private
     pdmax : Integer;
     pdmin : Integer;
     pprob : real;

     published
     constructor Create(pdmax:integer; pdmin : integer);
     property dmax : integer read pdmax write pdmax;
     property dmin : integer read pdmin write pdmin;
     property prob : real read pprob write pprob;


end;
   implementation
   constructor
   begin
          self.dmax := pID;
          self.dmin := pxcoor;
          self.prob := pycoor;
   end;
end.

